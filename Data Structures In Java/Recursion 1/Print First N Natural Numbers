public class Solution {

	public static void print(int n){
		//Write your code here
		
		// Base case: if n is 1, print 1 and return
		if (n == 1) {
			System.out.print(1 + " ");
			return;
		}
		// Recursive case: print numbers from 1 to n-1 first, then print n
		print(n-1);
		System.out.print(n + " ");
		
	}
}
