import java.util.Scanner;

public class Solution {
    public static void main(String args[]) {
        Scanner sc = new Scanner(System.in);
        int test = sc.nextInt();

        for (int i = 0; i < test; i++) {
            int P = sc.nextInt();
            int R = sc.nextInt();
            int T = sc.nextInt();
            double SI = (P * R * T) / 100.0;
            System.out.printf("%.2f\n", SI);
        }
    }
}
