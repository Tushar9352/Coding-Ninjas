import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int squareRoot = 0;
        for (int i = 1; i <= n; i++) {
            if (i * i <= n) {
                squareRoot = i;
            } else {
                break;
            }
        }
        System.out.println(squareRoot);
    }
}
